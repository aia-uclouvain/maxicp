<instance format="XCSP3" type="CSP">
  <variables>
    <array id="x" size="[5][5]"> 1..15 </array>
  </variables>
  <constraints>
    <allDifferent> x[0][] x[1][0..3] x[2][0..2] x[3][0..1] x[4][0] </allDifferent>
    <group>
      <intension> eq(%0,dist(%1,%2)) </intension>
      <args> x[1][0] x[0][0..1] </args>
      <args> x[1][1] x[0][1..2] </args>
      <args> x[1][2] x[0][2..3] </args>
      <args> x[1][3] x[0][3..4] </args>
      <args> x[2][0] x[1][0..1] </args>
      <args> x[2][1] x[1][1..2] </args>
      <args> x[2][2] x[1][2..3] </args>
      <args> x[3][0] x[2][0..1] </args>
      <args> x[3][1] x[2][1..2] </args>
      <args> x[4][0] x[3][0..1] </args>
    </group>
    <intension class="symmetry-breaking"> lt(x[3][0],x[3][1]) </intension>
  </constraints>
</instance>
